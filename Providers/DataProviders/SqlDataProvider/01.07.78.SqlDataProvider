			(select count(*) from {databaseOwner}{objectQualifier}Ventrian_PropertyAgent_Statistic s where p.PropertyID = s.PropertyID AND s.[DateCreated] > DateAdd(dd, -30, GetDate())) as 'ViewCount',
			p.[Status], 
